programa {
  funcao inicio() {
    
    //Criar um algoritmo para mostrar a tabuada do numero 8
    // utilizando a estrutura de repetição enquanto

    inteiro num,n1
    escreva("Digite o número que deseja ver a tabuada: ")
    leia(n1)
    num =1

    enquanto(num <= 10){
        escreva(num, " X ", n1 , " = " , num * n1, "\n")
        
        num++

    }


  }
}
