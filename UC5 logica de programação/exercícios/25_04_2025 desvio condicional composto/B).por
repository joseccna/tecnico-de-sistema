programa {
  funcao inicio() {

    escreva("Digite qualquer numero inteiro!")
    escreva("\n")
    inteiro num1
    escreva("Digite o numero: ")
    leia(num1)
    se(num1 >= 0){
      escreva("O numero ", num1, " é positivo!")
    } senao{
      escreva("O numero ", num1, " é negativo!")
    }


    
  }
}