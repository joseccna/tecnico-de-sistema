programa {
  funcao inicio() {

    escreva("Digite dois valores!")
    escreva("\n")
    inteiro num1, num2, maior
    escreva("Digite o primeiro numero: ")
    leia(num1)
    escreva("Digite o segundo numero: ")
    leia(num2)
    se(num1>num2){
      escreva("O primeiro numero ", num1, " é o maior numero")
    } senao{
      escreva("O segundo numero ", num2 ," é maior numero")
    }


    
  }
}